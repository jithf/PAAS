<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jit.edu.paas.mapper.SysLoginMapper">
    <select id="hasExist" resultType="java.lang.Boolean">
        SELECT COUNT(*) FROM `sys_login` WHERE id = #{id}
    </select>
    <select id="listId" resultType="java.lang.String">
        SELECT id from `sys_login`
    </select>
</mapper>
